  "main": "index.js",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "npx nodemon server.ts",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:push": "npx dotenv -e .env -- prisma db push",
    "db:seed": "npx dotenv -e .env -- prisma db seed",
    "migrate:dev": "npx prisma generate dev",
    "migrate:reset": "npx  prisma generate reset",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx dotenv -e .env -- prisma studio",
    "production:build": "npx prisma generate && npx prisma generate deploy"
  },
